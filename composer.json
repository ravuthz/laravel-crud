{
    "name": "ravuthz/laravel-crud",
    "description": "A Laravel package providing a base controller for CRUD operations.",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/console": "^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2"
    },
    "autoload": {
        "psr-4": {
            "Ravuthz\\LaravelCrud\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "authors": [
        {
            "name": "Ravuthz",
            "email": "ravuthz@gmail.com"
        }
    ],
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "Ravuthz\\LaravelCrud\\CrudServiceProvider"
            ]
        }
    }
}